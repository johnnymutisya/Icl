<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffba1299"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:padding="20.0dip">

    <TextView
        android:id="@id/textView2"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10.0dip"
        android:gravity="center_horizontal"
        android:text="JIINUE GROUPS REPORTS"
        android:textAllCaps="false"
        android:textColor="@color/colorAccent"
        fancy:layout_constraintLeft_toLeftOf="parent"
        fancy:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@id/textView5"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Please Select Your Category"
        android:textColor="@color/colorAccent"
        fancy:layout_constraintLeft_toLeftOf="parent"
        fancy:layout_constraintRight_toRightOf="parent" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNparentsreports"
        android:layout_width="300.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="Jiinue Members Reports"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNctreports"
        android:layout_width="300.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="Jiinue Group Admins Reports"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNdeanreports"
        android:layout_width="300.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="Jiinue Group Leaders Reports"
        fancy:fb_textColor="#ffffffff" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:id="@+id/BTNprincipalreports"
        android:layout_width="300.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.0dip"
        android:paddingBottom="10.0dip"
        android:paddingLeft="20.0dip"
        android:paddingRight="20.0dip"
        android:paddingTop="10.0dip"
        fancy:fb_borderColor="#ffffffff"
        fancy:fb_borderWidth="1.0dip"
        fancy:fb_defaultColor="@color/colorButtonDark"
        fancy:fb_focusColor="@color/colorButtonLight"
        fancy:fb_iconPosition="left"
        fancy:fb_radius="6.0dip"
        fancy:fb_text="Jiinue Group Director Reports"
        fancy:fb_textColor="#ffffffff" />
</LinearLayout>