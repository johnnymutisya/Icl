    <Button
        android:id="@+id/button2"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:text="Button" />

    <Button
        android:id="@+id/button"
        android:layout_width="522.0dip"
        android:layout_height="wrap_content"
        android:text="TOTAL NUMBER OF LEARNERS IN THE COUNTRY" />

    <Button
        android:id="@+id/BTNtla"
