<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <TextView
        android:id="@id/textView2"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="COUNTY DIRECTOR REPORTS"
        android:textAllCaps="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@id/textView5"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="REPORTS GENERATED REAL TIME !!!!!!!!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@id/BTNtotalclassenrollment"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="Total LEARNER Enrollment IN EACH SUBCOUNTY" />

    <Button
        android:id="@id/BTNtotalstreamenrolment"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="TOTAL COUNTY LEARNER ENROLMENT IN ALL THE SCHOOLS" />

    <Button
        android:id="@+id/BTNtotalschoolenrolment"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="TOTAL TEACHERS FOR EACH SUBJECT COMBINATION IN ALL SCHOOLS" />

    <Button
        android:id="@+id/BTNstreamexamperformance"
        android:layout_width="368.0dip"
        android:layout_height="wrap_content"
        android:text="TOTAL MINISTRY TEXTBOOKS RECIEVED BY EACH SCHOOL" />
</LinearLayout>